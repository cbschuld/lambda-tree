{
  "name": "lambda-tree",
  "version": "1.0.0",
  "description": "Lambda Tree - a source for logs.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npm run tsc",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbschuld/lambda-tree.git"
  },
  "keywords": [
    "log",
    "logging",
    "logger",
    "logs",
    "lambda",
    "lambda logging"
  ],
  "author": "Chris Schuld <cbschuld@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cbschuld/lambda-tree/issues"
  },
  "homepage": "https://github.com/cbschuld/lambda-tree#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.1.1",
    "luxon": "^2.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  }
}
